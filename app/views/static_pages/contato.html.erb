<% provide(:title, "Contato") %>
<div class="col-md-2 column">
</div>
<div class="col-md-8 column">
      <br>
<legend>Contato</legend>

<form role="form" action="#" method="POST">
  
    <div class="form-group">
      <label for="firstname" class="col-sm-2 control-label">Nome</label>
      <div class="col-sm-10">
         <input type="text" class="form-control" id="firstname" 
            placeholder="Nome">
      </div>
   </div>
   <br><br><br>
   <div class="form-group">
      <label for="lastname" class="col-sm-2 control-label">Email</label>
      <div class="col-sm-10">
         <input type="text" class="form-control" id="lastname" 
            placeholder="Email">
      </div>
   </div>
   
    <br><br>
  <div class="form-group">
   <label for="firstname" class="col-sm-2 control-label">Mensagem</label>
      <div class="col-sm-10">
    <textarea class="form-control" rows="3" placeholder="Digite sua Mensagem..."></textarea>
  </div>
     </div>
  <div class="form-group">
  <label for="controls" for="enviar"></label>
  <div class="controls">
    <button id="enviar" name="enviar" class="btn btn-primary">Enviar</button>
    <button id="cancelar" name="cancelar" class="btn btn-warning">Limpar</button>
  </div>
</div>
  
</form>


</div>
<div class="col-md-2 column">
</div>
